    zj.rwfk_id zjid ,
    un.ssejdwmc ssejdwmc,
    decode(zj.rwfk_id,null,'0','1') writed,
    cgid, cglx, xm, xb, csrq, csd, hjszd, sfzh, ssejdw, ks, zw, zc, lxdh, email, nncfcs, qtshrz,
