    status, shjg, shry, shyj, shrq, gdwjid, cfzjbgid, valid, create_time,'',operator
    from t_cg_dqcgj
    left join (select rwfk_id ,cgid zjsbid  from t_cg_rwzxqkfk ) zj on cgid = zj.zjsbid
